  <form id="manually_enter" method="post" action="/directions">
    <input type="hidden" name="baseURL" value="https://maps.googleapis.com/maps/api/directions/json?">

    <label for="origin">Start: </label>

    <!-- <input type="text" name="origin" value="633 Folsom 94107">&nbsp;&nbsp; -->
