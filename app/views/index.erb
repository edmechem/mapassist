    <label for="destination">Dest: </label>

    <!-- <input type="text" name="destination" value="1745 Folsom 94103">&nbsp;&nbsp; -->
    <input type="text" name="destination" value="Macarthur Station 94609">&nbsp;&nbsp;

    Mode: <select name="mode">
      <option value="driving">Driving</option>
      <option value="bicycling">Bicycling</option>
      <option value="walking">Walking</option>
      <option value="transit" selected>Transit</option>
    </select>&nbsp;&nbsp;&nbsp;&nbsp;
    <input type="submit" value="Directions">
  </form>
